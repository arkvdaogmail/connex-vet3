{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "api/prepaid-tx.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.html"
    },
    {
      "src": "/api/prepaid-tx",
      "dest": "api/prepaid-tx.js"
    }
  ]
}
